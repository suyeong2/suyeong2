data A;
input X Y $;
cards;
1 one
2 two
2 two
3 three
;
run;

data B;
input X Z $;
cards;
1 one
2 two
4 four
;
run;

proc sort data=A;
by X;
quit;

proc sort data=B;
by X;
quit;

data test;
merge A B;
by X;
quit;


/*join*/
data A;
input X Y $;
cards;
1 one
2 two
2 two
3 three
;
run;

data B;
input X Z $;
cards;
1 one
2 two
4 four
;
run;


proc sql;
create table t1 as
select * from A
join B on
A.X=B.X;
quit;

proc sql;
create table t2 as
select *
from A
full join B on
A.X=B.X;
quit;

proc sql;
create table t3 as
select * 
from A
left join B on
A.X=B.X;
quit;

proc sql;
create table t4 as
select *
from A
right join B on
A.X=B.X;
quit;

